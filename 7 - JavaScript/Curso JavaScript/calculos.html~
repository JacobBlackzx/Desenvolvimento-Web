<!Doctype html>
<html lang="pt-br">
	<head>
		<title>Calculos JavaScript</title>
		<meta charset="utf-8" />
		
		<script type="text/javascript" >
			
			// parseInt
			
			// parsefloat
			
//			var num1 = 9;
//			var num2 = 2;

			var num1 = prompt('Digite o valor A:');
			var num2 = prompt('Digite o valor B:');
			
//			num1 = parseInt(num1);
//			num2 = parseInt(num2);
			
			num1 = parseFloat(num1);
			num2 = parseFloat(num2)
			
			
			// Soma (+)
			document.write('A soma entre ' + num1 + ' e ' + num2 + ' é: ' + (num1 + num2) + '<br /><br />');
			
			// Subtração (-)
			document.write('A subtração ' + num1 + ' e ' + num2 + ' é ' + (num1 - num2) + '<br /><br />');
			
			// Multiplicação (*)
			document.write('A multiplicação ' + num1 + ' e ' + num2 + ' é ' + (num1 * num2) + '<br /><br />');
			
			// Divisão
			document.write('A divisão ' + num1 + ' e ' + num2 + ' é ' + (num1 / num2) + '<br /><br />');
			
			// Módulo
			document.write('A módulo ' + num1 + ' e ' + num2 + ' é ' + (num1 % num2) + '<br /><br />');
			
			// Incremento
			document.write('A incremento ' + num1 + ' é ' + (++num1) + '<br /><br />');
			
			// Decremento
			document.write('A incremento ' + num1 + ' é ' + (--num1) + '<br /><br />');
			
			
			
		</script>
		
	</head>
	
	<body>
	
	</body>
</html>